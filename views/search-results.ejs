
<%- include("./partials/head.ejs") %>
<%- include("./partials/header.ejs") %>

<body>

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de b√∫squeda</h2>
			</div>
		

			<!-- Show the "ultimos" products --> 

			<% for( let i = 0; i < 1; i++ ) { %>
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/product/<%= Muestra[i].id %>">
							<figure class="product-box_image">
								<img src=<%= Muestra[i].imagen %> alt="cafetera moulinex">
							</figure>
							<article class="product-box_data">
								<h2><%= Muestra[i].producto %></h2>
								<p><%= Muestra[i].descripcion %></p>
								
							</article> 
						
						</a> 
					</section>
				</div>
				<% } %>
			</div>
		</div>

<%- include("./partials/footer.ejs") %>

</body>

</html>